{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["html"],
  "rules": {
    "import/prefer-default-export": 0,
    "new-cap": 0,
    "camelcase": 0,
    "react/forbid-prop-types": 0,
    "arrow-parens": 0
  }
}
